{
    "version": 2,
    "name": "Socket",
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/v2/(.*)", 
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*", 
          "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE", 
          "Access-Control-Allow-Headers": "Content-Type,Authorization" 
        },
        "dest": "/index.js" 
      },
      {
        "src": "/(.*)", 
        "dest": "/index.js" 
      }
    ]
  }
  